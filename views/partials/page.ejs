<div class="page__style contact">
    <div class="page__description">
        <div id="contact">
            <div class="contact">
                <div class="load gmail" style="display: none">
                    <% include partials/loading %>
                </div>
                <form  style="display: block" action="/cntact" id="contact-form" method="post" role="form">
                    <div class="toolbar-form">
                        <p>Nouveau message</p>
                        <button><i class="material-icons">attachment</i></button>
                        <button id="select_link"  type="submit"><i class="material-icons">send</i></button>

                    </div>
                    <fieldset>
                        <div class="form-group">
                            <label for="email">De</label>
                            <input
                                    id="mail"
                                    name="mail"
                                    type="text"
                                    placeholder="Your email"
                                    value="tidjol@gmail.com"
                                    required>
                        </div>
                        <div class="form-group">
                            <label for="email">À</label>
                            <input id="mail" name="mail" type="text" placeholder="Your email" value="tidjol@gmail.com" required>
                        </div>
                        <div class="form-group">
                            <label for="">Objet</label>
                            <input type="text" value="Say hello ! :D">
                        </div>
                        <textarea id="msg" name="msg" placeholder="Rédigez votre message" rows="3"  required></textarea>




                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>
