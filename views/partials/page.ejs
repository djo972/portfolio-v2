<div class="page__style projects">
    <div class="page__description">
        <div id="projects">
            <div id="timeline-insta"  style="display: none">
                <% for (var key in insta) { %>
                    <div class="card-insta">
                        <div class="insta-header">
                            <img   src="<%= insta[key].user.profile_picture%>"  alt="">
                            <div class="user">
                                <p><%=insta[key].user.username%></p>

                                <div class="location">
                                    <%if (insta[key].location != null) { %>
                                    <%=insta[key].location.name%>
                                    <% } %>
                                </div>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="insta-content">
                            <img   src="<%= insta[key].images.standard_resolution.url%>"  alt="">
                        </div>
                        <div class="insta-footer">
                            <p> <%= insta[key].likes.count%>  ❤️</p>
                            <div class="tag">
                                <p><%=insta[key].user.username%> <span> <%=insta[key].caption.text%></span></p>
                            </div>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</div>
